{
  "expo": {
    "name": "Cinematch",
    "slug": "Cinematch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cinematch-logo.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Bungee-Regular.ttf"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cinematch-logo.png",
        "backgroundColor": "#9A0315"
      },
      "package": "com.dred_u.Cinematch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "03083002-7b07-4311-830d-64ab59ea904c"
      }
    }
  }
}
